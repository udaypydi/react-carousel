{
  "name": "@brainhubeu/gatsby-docs-kit-start",
  "description": "Gatsby-Docs-Kit starter repository",
  "version": "0.0.8",
  "author": "Brainhub",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf .cache && rm -rf public && gatsby build --prefix-paths",
    "deploy:gh": "bash tools/deploy-github.sh",
    "develop": "rm -rf .cache && gatsby develop",
    "seed": "bash tools/seed.sh",
    "serve": "gatsby serve",
    "lint": "eslint --ext .jsx,.js .",
    "lint:autofix": "eslint --ext .jsx,.js . --fix"
  },
  "dependencies": {
    "@brainhubeu/gatsby-docs-kit": "^1.0.3",
    "@brainhubeu/react-carousel": "^1.10.20",
    "gatsby": "^2.15.28",
    "react-fa": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint": "^6.5.0",
    "eslint-config-brainhub": "^1.11.0",
    "gh-pages": "^2.1.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  }
}
